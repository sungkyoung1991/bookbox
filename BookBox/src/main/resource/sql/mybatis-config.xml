		<typeAlias alias="chatRoom" type="com.bookbox.service.domain.ChatRoom"/>
		<typeAlias alias="comment" type="com.bookbox.service.domain.Comment"/>
		<typeAlias alias="creation" type="com.bookbox.service.domain.Creation"/>
		<typeAlias alias="funding" type="com.bookbox.service.domain.Funding"/>
		<typeAlias alias="payInfo" type="com.bookbox.service.domain.PayInfo"/>
		<typeAlias alias="posting" type="com.bookbox.service.domain.Posting"/>
		<typeAlias alias="writing" type="com.bookbox.service.domain.Writing"/>
		<typeAlias alias="book" type="com.bookbox.service.domain.Book"/>
		<typeAlias alias="recommend" type="com.bookbox.service.domain.Recommend"/>
		<typeAlias alias="report" type="com.bookbox.service.domain.Report"/>
	

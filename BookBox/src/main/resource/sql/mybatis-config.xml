	<mappers>
		<mapper resource="sql/UserMapper.xml"/>
		<mapper resource="sql/CommunityMapper.xml"/>
		<mapper resource="sql/PostingMapper.xml"/>
		<mapper resource="sql/CommonMapper.xml"/>
